{
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e", "preview"]
			}
		}
	},
	"namedInputs": {
		"noMarkDown": ["!{projectRoot}/src/**/*.{md,spec.ts}"]
	},
	"targetDefaults": {
		"build": {
			"inputs": ["noMarkDown"],
			"dependsOn": ["^build"],
			"outputs": ["build/**", "dist/**", ".vercel_build_output/**", ".svelte-kit/**"]
		},
		"dev": {
			"inputs": ["noMarkDown"],
			"dependsOn": ["^build"]
		},
		"test": {
			"inputs": ["noMarkDown"]
		}
	}
}
